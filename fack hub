fackTab:AddButton({
	Name = "esp item",
	Callback = function()
-- ตรวจสอบว่ามี Model ชื่อ Key1 อยู่ใน Workspace หรือไม่
local modelName = "Key1"
local model = workspace:FindFirstChild(modelName)

if model then
    print(modelName .. " exists in the Workspace.")

    -- ตรวจสอบว่ามี Highlights เดิมอยู่หรือไม่ ถ้ามีก็ลบทิ้ง
    local oldHighlight = model:FindFirstChildOfClass("Highlight")
    if oldHighlight then
        oldHighlight:Destroy()
    end

    -- สร้าง Highlights ใหม่
    local highlight = Instance.new("Highlight")
    highlight.Name = "Key1Highlight"  -- ตั้งชื่อ Highlights
    highlight.Parent = model           -- กำหนด parent เป็น Model ที่ชื่อ Key1
    highlight.FillTransparency = 0.8   -- ความโปร่งใสของพื้นที่ข้างใน
    highlight.FillColor = Color3.new(0, 1, 0)  -- สีพื้นที่ข้างใน (สีเขียว)
    highlight.OutlineColor = Color3.new(0, 1, 0) -- สีของเส้นขอบ (สีเขียว)
    highlight.OutlineTransparency = 0 -- ความโปร่งใสของเส้นขอบ

    -- ตรวจสอบว่ามี ESP Text (BillboardGui) เดิมอยู่หรือไม่ ถ้ามีก็ลบทิ้ง
    local oldESP = model:FindFirstChild("ESPText")
    if oldESP then
        oldESP:Destroy()
    end

    -- สร้าง BillboardGui ใหม่สำหรับ ESP Text
    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Name = "ESPText" -- ตั้งชื่อ BillboardGui
    billboardGui.Parent = model -- กำหนด parent เป็น Model ที่ชื่อ Key1
    billboardGui.AlwaysOnTop = true -- ให้แสดงผลตลอดเวลา
    billboardGui.Size = UDim2.new(0, 25, 0, 25) -- ขนาดของ GUI
    billboardGui.StudsOffset = Vector3.new(0, 3, 0) -- ย้ายตำแหน่ง GUI เหนือ Model

    -- สร้าง TextLabel สำหรับแสดงชื่อ Model
    local textLabel = Instance.new("TextLabel")
    textLabel.Parent = billboardGui -- กำหนด parent เป็น BillboardGui
    textLabel.Size = UDim2.new(1, 0, 1, 0) -- ขนาดของ TextLabel
    textLabel.BackgroundTransparency = 1 -- พื้นหลังโปร่งใส
    textLabel.Text = Key -- แสดงชื่อของ Model
    textLabel.TextColor3 = Color3.new(0, 1, 0) -- สีของข้อความเป็นสีเขียว
    textLabel.TextScaled = true -- ปรับขนาดตัวอักษรให้พอดีกับ TextLabel

else
    print(modelName .. " does not exist in the Workspace.")
end

local function addESPToButterflies()
    local workspace = game:GetService("Workspace") -- เข้าถึง Workspace
    local butterflies = {} -- สร้างตารางเก็บโมเดล Butterfly

    -- ค้นหาโมเดลชื่อ "Butterfly" ทั้งหมดใน Workspace
    for _, model in ipairs(workspace:GetChildren()) do
        if model:IsA("Model") and model.Name == "Butterfly" then
            table.insert(butterflies, model) -- เพิ่มโมเดลลงในตาราง
        end
    end

    -- สำหรับแต่ละโมเดลที่เจอ
    for _, butterfly in ipairs(butterflies) do
        -- ถ้ามี ESP Text อันเดิมอยู่ให้ลบทิ้ง
        local existingESP = butterfly:FindFirstChild("ESPText")
        if existingESP then
            existingESP:Destroy()
        end

        -- สร้าง ESP Text ใหม่
        local espText = Instance.new("BillboardGui")
        espText.Name = "ESPText"
        espText.Adornee = butterfly.PrimaryPart -- ทำให้ ESP ชี้ไปที่ส่วนหลักของโมเดล
        espText.Size = UDim2.new(0, 25, 0, 25) -- ขนาดของ ESP
        espText.AlwaysOnTop = true -- ให้ ESP อยู่ข้างบนตลอดเวลา

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1 -- ปรับความโปร่งใสของพื้นหลัง
        textLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- สีเขียว
        textLabel.TextScaled = true -- ปรับขนาดข้อความอัตโนมัติ
        
        -- แสดงชื่อเป็น "Butterfly" สำหรับทุกโมเดล
        textLabel.Text = "Butterfly"
        
        -- เพิ่ม TextLabel ลงใน BillboardGui
        textLabel.Parent = espText
        espText.Parent = butterfly -- เพิ่ม BillboardGui ลงในโมเดล
    end

    if #butterflies > 0 then
        print("ได้เพิ่ม ESP Text ให้กับโมเดล 'Butterfly'")
    else
        print("ไม่มีโมเดลชื่อ 'Butterfly' ใน Workspace")
    end
end

-- เรียกใช้งานฟังก์ชัน
addESPToButterflies()
        end  
	})

