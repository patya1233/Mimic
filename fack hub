bTab:AddButton({
	Name = "esp item",
	Callback = function()
local function createHighlight(handle)
    local highlight = Instance.new("Highlight")
    highlight.Name = "Highlight"
    highlight.Parent = handle
    highlight.FillColor = Color3.new(0, 1, 0) -- สีเขียว
    highlight.FillTransparency = 0.5 -- ความโปร่งใส
    highlight.OutlineColor = Color3.new(0, 1, 0) -- สีเขียวสำหรับเส้นขอบ
    highlight.OutlineTransparency = 0 -- ความโปร่งใสของเส้นขอบ
end

local function createESP(giver, handle, height)
    local espText = Instance.new("BillboardGui")
    espText.Name = "ESP"
    espText.Size = UDim2.new(1, 0, 1, 0)
    espText.Adornee = handle
    espText.AlwaysOnTop = true

    -- ปรับตำแหน่ง ESP ให้สูงขึ้น
    espText.StudsOffset = Vector3.new(0, height, 0) -- กำหนดความสูงที่ต้องการ

    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(0, 25, 0, 25)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = giver.Name
    textLabel.TextColor3 = Color3.new(0, 1, 0) -- สีเขียว
    textLabel.TextScaled = true
    textLabel.Parent = espText

    espText.Parent = handle
end

local giversFolder = game.Workspace:FindFirstChild("Givers")

if giversFolder then
    local espHeight = 2 -- ตั้งค่าความสูงของ ESP ที่นี่

    for _, giver in pairs(giversFolder:GetChildren()) do
        local handle = giver:FindFirstChild("Handle")

        if handle then
            -- ลบ Highlight ที่มีอยู่แล้ว
            local existingHighlight = handle:FindFirstChildOfClass("Highlight")
            if existingHighlight then existingHighlight:Destroy() end
            
            createHighlight(handle)

            -- ลบ ESP ที่มีอยู่แล้ว
            local existingESP = handle:FindFirstChild("ESP")
            if existingESP then existingESP:Destroy() end

            createESP(giver, handle, espHeight) -- ส่งค่าความสูงไปยังฟังก์ชัน createESP

            print(giver.Name .. " ได้เพิ่ม Highlight และ ESP ใหม่ให้กับ Handle")
        else
            print(giver.Name .. " ไม่มี Handle")
        end
    end
else
    print("ไม่มีโฟลเดอร์ Givers ใน Workspace")
end

local part = workspace:FindFirstChild("Rose") -- ค้นหา Part ที่ชื่อ Rose ใน Workspace

if part then
    local handle = part:FindFirstChild("Handle") -- ค้นหา Object ที่ชื่อ Handle ภายใน Rose
    if handle then
        print("Found Handle inside Rose")
        
        -- ลบ Highlight และ BillboardGui ที่มีอยู่เดิม ถ้ามี
        local oldHighlight = handle:FindFirstChild("Highlight")
        if oldHighlight then
            oldHighlight:Destroy()
        end
        
        local oldBillboardGui = handle:FindFirstChild("BillboardGui")
        if oldBillboardGui then
            oldBillboardGui:Destroy()
        end

        -- เพิ่ม Highlight ใหม่
        local highlight = Instance.new("Highlight")
        highlight.Adornee = handle
        highlight.Parent = handle
        highlight.FillTransparency = 1 -- ทำให้โปร่งใสข้างใน
        highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
        highlight.OutlineTransparency = 0 -- เส้นขอบไม่โปร่งใส

        -- เพิ่ม ESP Text (BillboardGui) ใหม่
        local billboardGui = Instance.new("BillboardGui")
        billboardGui.Parent = handle
        billboardGui.Adornee = handle
        billboardGui.Size = UDim2.new(0, 25, 0, 25) -- ขนาดของ BillboardGui
        billboardGui.StudsOffset = Vector3.new(0, 2, 0) -- ยกขึ้นมาด้านบน Handle
        billboardGui.AlwaysOnTop = true -- ทำให้ข้อความอยู่ด้านบนเสมอ

        local textLabel = Instance.new("TextLabel")
        textLabel.Parent = billboardGui
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1 -- ทำให้ไม่มีพื้นหลัง
        textLabel.Text = "Rose"
        textLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- สีเขียว
        textLabel.TextTransparency = 0 -- ไม่โปร่งใส
        textLabel.TextScaled = true -- ทำให้ขนาดตัวอักษรปรับอัตโนมัติตามขนาดกล่อง

    else
        print("Handle not found inside Rose")
    end
else
    print("Part 'Rose' not found in Workspace")
end

-- ค้นหา Holder ทั้งหมดใน Workspace
local holders = game.Workspace:GetChildren()

for _, holder in ipairs(holders) do
    -- ตรวจสอบว่าเป็น Holder หรือไม่
    if holder:IsA("Model") and holder.Name == "Holder" then
        -- ลบ Highlight เก่า
        local oldHighlight = holder:FindFirstChildOfClass("Highlight")
        if oldHighlight then
            oldHighlight:Destroy()
        end

        -- สร้าง Highlight ใหม่
        local highlight = Instance.new("Highlight")
        highlight.Parent = holder
        highlight.FillTransparency = 1 -- ข้างในโปร่งใส
        highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- เส้นขอบสีเขียว
        highlight.OutlineTransparency = 0 -- เส้นขอบไม่โปร่งใส

        -- ลบ BillboardGui (ESP Text) เก่า
        local oldBillboard = holder:FindFirstChild("HolderBillboard")
        if oldBillboard then
            oldBillboard:Destroy()
        end

        -- สร้าง BillboardGui (ESP Text) ใหม่
        local billboardGui = Instance.new("BillboardGui")
        billboardGui.Name = "HolderBillboard"
        billboardGui.Adornee = holder -- ทำให้ BillboardGui ยึดกับ Holder
        billboardGui.Size = UDim2.new(0, 25, 0, 25)
        billboardGui.StudsOffset = Vector3.new(0, 3, 0) -- ยกขึ้นเล็กน้อย
        billboardGui.AlwaysOnTop = true -- ให้แสดงผลตลอดเวลา
        billboardGui.Parent = holder

        -- เพิ่ม TextLabel สำหรับ ESP Text
        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = "Holder"
        textLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- สีเขียว
        textLabel.TextScaled = true
        textLabel.Parent = billboardGui

        print("Highlight and ESP Text added to " .. holder.Name)
    end
end
        end  
	})

