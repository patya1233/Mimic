	Name = "esp item",
	Callback = function()
local keyModel = game.Workspace:FindFirstChild("Key1") -- ค้นหา Model ที่ชื่อ Key1

if keyModel and keyModel:IsA("Model") then
    local handle = keyModel:FindFirstChild("Handle") -- ค้นหา Handle ใน Model Key1
    
    if handle then
        -- ตรวจสอบและจัดการ Highlight
        local oldHighlight = handle:FindFirstChildOfClass("Highlight")
        if oldHighlight then
            oldHighlight:Destroy() -- ลบ Highlight อันเดิมถ้ามี
        end
        
        -- เพิ่ม Highlight ใหม่
        local highlight = Instance.new("Highlight")
        highlight.Parent = handle
        highlight.FillColor = Color3.new(0, 1, 0) -- สีข้างในเป็นสีเขียว
        highlight.FillTransparency = 0.5 -- โปร่งใสบางส่วน
        highlight.OutlineColor = Color3.new(0, 1, 0) -- สีขอบสีเขียว
        highlight.OutlineTransparency = 0 -- ขอบไม่โปร่งใส

        print("เพิ่ม Highlight ให้กับ Handle ใน Key1 แล้ว")
    else
        print("ไม่พบ Handle ใน Model Key1")
    end
else
    print("ไม่พบ Model ที่ชื่อ Key1 ใน Workspace")
end

-- ฟังก์ชันสำหรับเพิ่ม Highlight ให้กับ MeshPart
local function addHighlight(meshPart)
    -- ตรวจสอบว่า MeshPart มี Highlight เดิมอยู่หรือไม่ ถ้ามีให้ลบทิ้ง
    local oldHighlight = meshPart:FindFirstChildOfClass("Highlight")
    if oldHighlight then
        oldHighlight:Destroy()  -- ลบ Highlight เดิม
    end

    -- สร้าง Highlight ใหม่
    local highlight = Instance.new("Highlight")
    highlight.Parent = meshPart  -- ตั้ง Parent เป็น MeshPart
    highlight.FillTransparency = 0.5  -- ปรับความโปร่งใสของ Fill
    highlight.FillColor = Color3.new(0, 1, 0)  -- สีเขียว (RGB: 0, 255, 0)
    highlight.OutlineColor = Color3.new(0, 1, 0)  -- สีเขียวสำหรับเส้นขอบ
    highlight.OutlineTransparency = 0  -- เส้นขอบโปร่งใสน้อยที่สุด
end

-- ฟังก์ชันสำหรับจัดการ CodeDoor แต่ละตัว
local function processCodeDoor(codeDoor)
    -- ใช้ GetDescendants() เพื่อดึง Object ทั้งหมดภายใน CodeDoor
    for _, descendant in pairs(codeDoor:GetDescendants()) do
        if descendant:IsA("MeshPart") then  -- ตรวจสอบว่าเป็น MeshPart หรือไม่
            addHighlight(descendant)  -- เพิ่ม Highlight ให้ MeshPart
            print("Highlight added to MeshPart: " .. descendant.Name)
        end
    end
end

-- ค้นหา Model ทั้งหมดที่ชื่อ CodeDoor ใน Workspace
for _, obj in pairs(workspace:GetChildren()) do
    if obj:IsA("Model") and obj.Name == "CodeDoor" then
        processCodeDoor(obj)  -- จัดการ Model แต่ละตัว
    end
end
        end  
	})

