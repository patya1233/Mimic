    for _, item in pairs(gameAIFolder:GetChildren()) do
        if item:IsA("Model") then  -- ตรวจสอบว่าเป็นโมเดลหรือไม่
            -- ลบ Highlight เดิมถ้ามี
            local existingHighlight = item:FindFirstChildOfClass("Highlight")
            if existingHighlight then
                existingHighlight:Destroy()
            end
            
            -- สร้าง Highlight ใหม่
            local highlight = Instance.new("Highlight")
            highlight.Name = "Highlight"
            highlight.Parent = item
            highlight.FillColor = Color3.new(1, 0, 0)  -- สีแดง
            highlight.FillTransparency = 0.5  -- โปร่งใส 50%
            highlight.OutlineColor = Color3.new(1, 0, 0)  -- สีแดง
            highlight.OutlineTransparency = 0  -- ไม่มีความโปร่งใสของขอบ
        end
    end
else
    print("ไม่พบโฟลเดอร์ GameAI ใน Workspace")
end
        end  
	})
end

