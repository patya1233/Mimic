	Name = "esp Monster",
	Callback = function()
local modelName = "omukadeMAIN"
local model = game.Workspace:FindFirstChild(modelName)

if model then
    -- ตรวจสอบและลบ Highlight ที่มีอยู่เดิม (ถ้ามี)
    local existingHighlight = model:FindFirstChildOfClass("Highlight")
    if existingHighlight then
        existingHighlight:Destroy()
    end

    -- สร้าง Highlight ใหม่
    local highlight = Instance.new("Highlight")
    highlight.Parent = model
    highlight.FillColor = Color3.fromRGB(255, 0, 0)  -- สีขอบแดง
    highlight.FillTransparency = 0.5  -- ความโปร่งใส (0 = ทึบ, 1 = โปร่งใส)
    highlight.OutlineColor = Color3.fromRGB(255, 0, 0)  -- สีขอบนอกแดง
    highlight.OutlineTransparency = 0  -- ขอบไม่โปร่งใส (0 = ไม่โปร่งใส)

    print("Highlight added to " .. modelName)
else
    print("Model " .. modelName .. " is not found in Workspace.")
end
        end  
	})

