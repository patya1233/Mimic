local Section = fackTab:AddSection({
	Name = "esp Hiachi"
})

fackTab:AddButton({
	Name = "esp all key",
	Callback = function()
-- ตรวจสอบว่าโฟลเดอร์ Givers มีอยู่ใน Workspace หรือไม่
local giversFolder = game.Workspace:FindFirstChild("Givers")

if giversFolder then
    -- รายชื่อ Key ที่ต้องการตรวจสอบ
    local keys = {"Key1", "Key2", "Key3"}

    -- วนลูปผ่าน Key ทั้งสาม
    for _, keyName in pairs(keys) do
        local key = giversFolder:FindFirstChild(keyName)

        if key then
            -- ตรวจสอบและลบ Highlight เก่าถ้ามี
            local existingHighlight = key:FindFirstChildOfClass("Highlight")
            if existingHighlight then
                existingHighlight:Destroy()
            end

            -- สร้าง Highlight ใหม่
            local highlight = Instance.new("Highlight")
            highlight.FillTransparency = 1 -- โปร่งใสข้างใน
            highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- ขอบสีเขียว
            highlight.Parent = key -- เพิ่ม Highlight ให้กับ Key

            -- ตรวจสอบและลบ BillboardGui (ESP Text) เก่าถ้ามี
            local existingESP = key:FindFirstChildOfClass("BillboardGui")
            if existingESP then
                existingESP:Destroy()
            end

            -- สร้าง BillboardGui ใหม่ (สำหรับ ESP Text)
            local esp = Instance.new("BillboardGui")
            esp.Name = "ESPText"
            esp.AlwaysOnTop = true -- ให้ตัวหนังสืออยู่ด้านบนเสมอ
            esp.Size = UDim2.new(0, 25, 0, 25) -- ขนาดของกล่องแสดงผล
            esp.StudsOffset = Vector3.new(0, 2, 0) -- ระยะห่างจากวัตถุ (เหนือขึ้นไป)

            -- สร้าง TextLabel สำหรับแสดงชื่อ Key
            local textLabel = Instance.new("TextLabel")
            textLabel.Size = UDim2.new(1, 0, 1, 0)
            textLabel.BackgroundTransparency = 1 -- พื้นหลังโปร่งใส
            textLabel.Text = keyName -- แสดงชื่อของ Key
            textLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- สีตัวอักษรสีเขียว
            textLabel.TextScaled = true -- ให้ข้อความขยายเต็มกรอบ

            -- เพิ่ม TextLabel เข้าไปใน BillboardGui
            textLabel.Parent = esp

            -- เพิ่ม BillboardGui เข้าไปใน Key
            esp.Parent = key
        else
            print(keyName .. " ไม่พบใน Givers")
        end
    end
else
    print("ไม่พบโฟลเดอร์ Givers ใน Workspace")
end
        end  
	})

fackTab:AddButton({
	Name = "esp Hiachi",
	Callback = function()
-- ตรวจสอบว่ามีโฟลเดอร์ GameAI ใน Workspace หรือไม่
local gameAIFolder = workspace:FindFirstChild("GameAI")

if gameAIFolder then
    -- ตรวจสอบว่ามีโมเดลชื่อ AI ในโฟลเดอร์ GameAI หรือไม่
    local aiModel = gameAIFolder:FindFirstChild("AI")
    
    if aiModel then
        -- ตรวจสอบว่ามี Highlight อันเดิมอยู่หรือไม่
        local existingHighlight = aiModel:FindFirstChildOfClass("Highlight")
        
        -- ถ้ามีให้ลบทิ้ง
        if existingHighlight then
            existingHighlight:Destroy()
        end
        
        -- สร้าง Highlight ใหม่
        local newHighlight = Instance.new("Highlight")
        newHighlight.Parent = aiModel
        
        -- ตั้งค่าของ Highlight
        newHighlight.FillColor = Color3.new(1, 0, 0) -- สีแดง (RGB: 1, 0, 0)
        newHighlight.FillTransparency = 0.5 -- โปร่งใส 100%
        newHighlight.OutlineColor = Color3.new(1, 0, 0) -- ขอบสีแดง
        
        print("เพิ่ม Highlight สีแดงให้โมเดล AI")
    else
        print("ไม่พบโมเดลชื่อ AI ในโฟลเดอร์ GameAI")
    end
else
    print("ไม่พบโฟลเดอร์ GameAI ใน Workspace")
end
        end  
	})
end

